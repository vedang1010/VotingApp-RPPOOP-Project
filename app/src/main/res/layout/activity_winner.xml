<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back79">

    <TextView
        android:id="@+id/winnerNameTextView"
        android:layout_width="365dp"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="10dp"
        android:text="WINNERS"
        android:textAlignment="center"
        android:textColor="#CBDA34"
        android:textSize="48sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/winnerNameTextView2"
        android:layout_width="287dp"
        android:layout_height="72dp"
        android:layout_below="@+id/winnerNameTextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="60dp"
        android:hint="Candidate Name"
        android:textAlignment="center"
        android:textColor="#CBDA34"
        android:textColorHint="#CBDA34"
        android:textSize="34sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/winnerNameTextView3"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_below="@+id/winnerNameTextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="217dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="21dp"
        android:hint="Election Name"
        android:textAlignment="center"
        android:textColor="#CBDA34"
        android:textColorHint="#CBDA34"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/voteCountTextView"
        android:layout_width="176dp"
        android:layout_height="70dp"
        android:layout_below="@+id/winnerNameTextView2"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="34dp"
        android:hint="Vote count"
        android:paddingStart="2sp"
        android:paddingTop="15sp"
        android:textAlignment="center"
        android:textColor="#CDDC39"
        android:textColorHint="#D4DF66"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="176dp"
        android:layout_height="70dp"
        android:layout_below="@+id/winnerNameTextView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="177dp"
        android:layout_toStartOf="@+id/voteCountTextView"
        android:paddingTop="5sp"
        android:paddingEnd="15sp"
        android:text="Vote Count"
        android:textAlignment="center"
        android:textColor="#DFEC63"
        android:textColorHint="#E1D9D9"
        android:textSize="28sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="167dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/winnerNameTextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="23dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="404dp"
        android:layout_toStartOf="@+id/voteCountTextView"
        android:paddingTop="5sp"
        android:paddingEnd="15sp"
        android:text="Election Name"
        android:textAlignment="center"
        android:textColor="#DFEC63"
        android:textColorHint="#E1D9D9"
        android:textSize="22sp" />

</RelativeLayout>
